{
  "name": "hello_world_example_with_tests",
  "version": "1.0.0",
  "description": "Hello World example with tests",
  "author": "Andrew Nease <andrew.nease.code@gmail.com",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel watch:server watch:build",
    "watch:build": "NODE_ENV=development webpack --watch",
    "watch:server": "nodemon app.js",
    "test": "mocha --require ignore-styles --require babel-core/register --require ./test/test-helpers.js --require ./test/mock-dom.js \"src/**/*.spec.js\" || true",
    "test-api": "mocha test/integration/api --api_url=sample.com || true"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "express": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-promise": "^0.5.3",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.4"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-redux": "^0.1.7",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "mocha": "^4.1.0",
    "sinon": "^4.1.6"
  }
}
